{
  "expo": {
    "scheme": "mapactionapp",
    "name": "Map Action",
    "slug": "MapActionApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/map-action.png",
      "resizeMode": "contain",
      "backgroundColor": "#2d9cdb"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker"
      ],
      [
        "expo-apple-authentication"
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.uwaish.MapActionAppv2",
      "buildNumber": "2",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1094350890225-6d469l4tlgojeqgel06okuf6n4fc968k"
            ]
          }
        ]
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.uwaish.MapActionApp",
      "config": {
        "cleartextTraffic": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fab32253-99ad-483e-b834-5b25aeaa68b4"
      }
    }
  }
}
